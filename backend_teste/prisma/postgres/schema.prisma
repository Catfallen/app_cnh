generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client-postgres"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE__URL")
}
model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  senha String

  @@map("usuario")
}